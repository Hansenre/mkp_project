{
  "name": "mkp",
  "version": "1.0.0",
  "description": "migrations - migração da nossa data base",
  "main": "app.js",
  "scripts": {
    "start": "node ./src/app.js",
    "dev": "nodemon ./src/app.js",
    "generate:migration": "npx sequelize-cli migration:generate --name",
    "create:migration": "npx sequelize-cli db:migrate",
    "undo:migration": "npx sequelize-cli db:migrate:undo"
